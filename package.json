{
  "name": "editor-egg",
  "version": "1.0.3",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=lego-backend",
    "stop": "egg-scripts stop --title=lego-backend",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean",
    "build:template": "npx webpack --config webpack/webpack.config.js",
    "upload": "node webpack/uploadToOSS.js",
    "build:template:dev": "npx webpack --config webpack/webpack.config.js",
    "build:template:prod": "npx webpack --config webpack/webpack.config.js --env production && npm run upload",
    "release": "release-it"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "dotenv": "^10.0.0",
    "egg": "^2.6.1",
    "egg-bcrypt": "^1.1.0",
    "egg-jwt": "^3.1.7",
    "egg-mongoose": "^3.3.1",
    "egg-oss": "^2.0.0",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.6.0",
    "egg-validate": "^2.0.2",
    "egg-view-nunjucks": "^2.3.0",
    "lego-bricks-react": "^0.0.9",
    "mongoose-sequence": "^5.3.1",
    "nanoid": "^3.1.30",
    "react-dom": "^17.0.2",
    "release-it": "^14.12.3"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/react-dom": "^17.0.11",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "filemanager-webpack-plugin": "^6.1.7",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "2.4.5",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
